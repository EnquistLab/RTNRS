Package: TNRS
Title: Taxonomic Name Resolution Service
Version: 0.3.0
Authors@R: 
    c(
            person("Brian", "Maitner", email = "bmaitner@gmail.com", role = c("aut", "cre"), comment = c(ORCID = "0000-0002-2118-9880")),
